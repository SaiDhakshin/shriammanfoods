<html>
    <head>
        <title>
            Send SMS
        </title>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
    </head>
    <body>
        <h1>Send SMS</h1>
        <form action="/send" method="POST">
        <ul>
            <li style="list-style: none;"><input type="text" name="username" placeholder="Enter Username"></li>
            <li style="list-style: none;"><input type="number" name="phonenumber" placeholder="Enter Phone number"></li>
            
        </ul>
        <button type="submit">Send</button>
        </form>
    </body>
</html>